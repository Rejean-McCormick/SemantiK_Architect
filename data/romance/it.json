{
  "meta": {
    "version": "1.0",
    "language": "it",
    "family": "romance"
  },

  "name": "Italian",

  "syntax": {
    "bio_default_tense": "present"
  },

  "articles": {
    "m": {
      "default": "un",
      "s_impure": "uno",
      "vowel": "un"
    },
    "f": {
      "default": "una",
      "vowel": "un'"
    }
  },

  "morphology": {
    "suffixes": [
      { "ends_with": "tore", "replace_with": "trice" },
      { "ends_with": "logo", "replace_with": "loga" },
      { "ends_with": "o", "replace_with": "a" }
    ],
    "irregulars": {
      "poeta": "poetessa",
      "duca": "duchessa",
      "re": "regina",
      "dio": "dea",
      "eroe": "eroina",
      "leone": "leonessa",
      "studente": "studentessa",
      "professore": "professoressa",
      "conte": "contessa"
    }
  },

  "phonetics": {
    "impure_triggers": [
      "s_consonant",
      "z",
      "gn",
      "ps",
      "pn",
      "x",
      "y"
    ],
    "elision": true,
    "stressed_a_words": []
  },

  "structure": "{name} Ã¨ {article}{sep}{profession} {nationality}."
}
