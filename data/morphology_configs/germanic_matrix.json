{
  "en": {
    "meta": {
      "family": "germanic",
      "language_code": "en",
      "name": "English"
    },
    "syntax": {
      "basic_word_order": "SVO",
      "allows_null_subject": false,
      "adjective_position": "pre",
      "genitive_strategy": "of_phrase",
      "relative_clause_position": "postnominal",
      "relative_marker": "that",
      "bio_default_tense": "past"
    },
    "articles": {
      "definite": {
        "singular": "the",
        "plural": "the"
      },
      "indefinite": {
        "singular": {
          "default": "a",
          "before_vowel": "an"
        },
        "plural": ""
      }
    },
    "nouns": {
      "gender_defaults": "common",
      "plural_rules": [
        {
          "ends_with": "y",
          "replace_with": "ies"
        },
        {
          "ends_with": "s",
          "replace_with": "ses"
        },
        {
          "ends_with": "",
          "replace_with": "s"
        }
      ],
      "irregular_plurals": {
        "person": "people",
        "man": "men",
        "woman": "women",
        "child": "children",
        "scientist": "scientists"
      }
    },
    "adjectives": {
      "degree": {
        "comparative_suffix": "er",
        "superlative_suffix": "est",
        "irregular": {
          "good": {
            "comparative": "better",
            "superlative": "best"
          },
          "bad": {
            "comparative": "worse",
            "superlative": "worst"
          }
        }
      },
      "position": "pre"
    },
    "verbs": {
      "copula": {
        "present": {
          "1sg": "am",
          "2sg": "are",
          "3sg": "is",
          "1pl": "are",
          "2pl": "are",
          "3pl": "are",
          "default": "is"
        },
        "past": {
          "1sg": "was",
          "2sg": "were",
          "3sg": "was",
          "1pl": "were",
          "2pl": "were",
          "3pl": "were",
          "default": "was"
        },
        "zero_present": false
      },
      "past_tense_rules": [
        {
          "ends_with": "e",
          "replace_with": "ed"
        },
        {
          "ends_with": "y",
          "replace_with": "ied"
        },
        {
          "ends_with": "",
          "replace_with": "ed"
        }
      ],
      "irregular_past": {
        "be": "was",
        "have": "had",
        "do": "did",
        "make": "made",
        "go": "went",
        "become": "became"
      }
    },
    "phonology": {
      "a_an_vowel_set": [
        "a",
        "e",
        "i",
        "o",
        "u"
      ]
    }
  },
