{
  "meta": {
    "version": "1.0",
    "language": "sv",
    "family": "germanic"
  },
  "name": "Swedish",

  "syntax": {
    "basic_word_order": "SVO",
    "allows_null_subject": false,
    "adjective_position": "pre",
    "relative_clause_position": "postnominal",
    "relative_marker": "som",
    "bio_default_tense": "present"
  },

  "articles": {
    "indefinite": {
      "m": "en",
      "f": "en",
      "n": "ett",
      "pl": ""
    }
  },

  "morphology": {
    "generic_feminine_suffix": "inna",
    "gender_suffixes": [
      { "ends_with": "are", "replace_with": "erska" },
      { "ends_with": "or", "replace_with": "ris" }
    ],
    "grammatical_gender_map": {
      "hus": "n",
      "bord": "n",
      "barn": "n"
    },
    "irregulars": {
      "skådespelare": "skådespelerska",
      "författare": "författarinna",
      "lärare": "lärarinna",
      "kung": "drottning",
      "prins": "prinsessa"
    }
  },

  "adjectives": {
    "inflects": true,
    "indefinite_endings": {
      "m": "",
      "f": "",
      "n": "t",
      "pl": "a"
    },
    "definite_endings": {
      "m": "a",
      "f": "a",
      "n": "a",
      "pl": "a"
    }
  },

  "verbs": {
    "copula": {
      "present": {
        "3sg": "är",
        "default": "är"
      },
      "past": {
        "3sg": "var",
        "default": "var"
      }
    }
  },

  "casing": {
    "capitalize_nouns": false
  },

  "structure": "{name} {copula} {article} {nationality} {profession}."
}
